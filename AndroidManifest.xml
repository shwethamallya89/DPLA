<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.android.mama_dpla"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

  <!--  <uses-permission android:name="android.permission.INTERNET" /> -->
    <!-- android:name="android.permission.WRITE_EXTERNAL_STORAGE" -->
    <uses-permission
        android:name="android.permission.INTERNET"
        android:maxSdkVersion="21" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <!--android:debuggable="true">-->

        <activity
            android:name="com.project.android.dpla.mama_dpla.StartActivity"
            android:label="@string/title_activity_start_act" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.project.android.dpla.mama_dpla.ResultActivity"
            android:label="@string/title_activity_result"
            android:parentActivityName="com.project.android.dpla.mama_dpla.StartActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.project.android.dpla.mama_dpla.StartActivity" />
        </activity>
    </application>

</manifest>
